{% extends "./layout.peb" %}

{% block title %}
Список проектов
{% endblock %}

{% block content %}
<h2>Список проектов</h2>
<ol class="h3">
    {% for project in model.projects %}
    <li>
        {{ project.value.name }} -
        <a href="projects/{{ project.index + 1 }}"> подробнее</a>
    </li>
    {% endfor %}
</ol>

{% endblock %}
